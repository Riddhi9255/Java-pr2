public class Person {
    private String name;
    private int age;
    private String address;

    // Constructor, getter and setter methods, and introduce method are same as previous implementation...

    public static void main(String[] args) {
        system.out.println("memdavadi riddhi , 220130318035");
        // Creating an instance of Person
        Person person = new Person("John", 30, "123 Main Street");

        // Using setter methods to set attributes
        person.setName("Alice");
        person.setAge(25);
        person.setAddress("456 Elm Street");

        // Calling the introduce method to print out the person's introduction message
        person.introduce();
    }
}
